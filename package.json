  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapbox/timespace/issues"
  },
  "homepage": "https://github.com/mapbox/timespace#readme",
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "strict": "off",
      "indent": [
        2,
        2
      ],
      "camelcase": 0,
      "space-before-function-paren": [
        2,
        "never"
      ]
    }
  }
